/* ошибки  */

struct error_info {
        num64 id  = 1;
	num64 code = 2;
}

struct error_ex_info {
        string cid = 1;
	num64  code = 2;
}

struct error_outside {
        num64 error = 1;
}

/* add_items( const vect<cid_key>&, vect<sid_key>&,  vect<error_info>&)    */


struct cid_key {
	num64  cid = 1;
	string name = 2;
	num64  type = 3;
	num64  dbound = 4;
}

struct sid_key {
	_num64 cid = 1;
	_num64 sid = 2;
}


struct req_add_items {
        array cid_key cids = 1;
}


struct resp_add_items {
	array sid_key     sids = 1;
	array error_item  errors = 2;
}




/* add_item_ex( const vect<cid_struct>&, vect<sid>&,  vect<error_item>&)    */

struct cid_key_ex {
	string cid = 1;
	num64  dbound = 2;
}

struct sid_key_ex {
	string cid = 1;
	num64  sid = 2;
	num64  val = 3;
	num64  type = 4;
	num64  time = 5;
	num64  vld = 6;
	num64  mineu = 7;
	num64  maxeu = 8;
	num64  alinf = 9;
	string valstr = 10;
	string comment = 11;
	string eu = 12;
}


struct req_add_items_ex {
        array cid_key_ex cids = 1;
}


struct resp_add_items {
	array cid_key_ex  sids = 1;
	array error_ex_info  errors = 2;
}





