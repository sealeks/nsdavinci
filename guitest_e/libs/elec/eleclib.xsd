        <xsd:appinfo>
            <lib:creator name="elib:switcher" hint="switcher">
                <elib:switcher x="" y="" height="50" width="50"/>
            </lib:creator>
        </xsd:appinfo>
    </xsd:annotation>        
    <xsd:complexType>
        <xsd:attribute name="id" type="xsd:ID" use="required"/>
        <xsd:attribute name="type" type="elib:switchertype"/>
