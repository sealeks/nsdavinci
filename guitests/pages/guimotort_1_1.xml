	<g xmlns:html="http://www.w3.org/TR/xhtml1" xmlns:mlib="http://dvnci/mlib" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="name12" xsi:schemaLocation="../mainlib/maillib.xsd">
		<svg x="20" y="20" height="40" width="200" state="on">
			<style type="text/css">
                
            g#name12 svg  text{
            fill: red;text-anchor: middle;
                 dominant-baseline: central; 
                 -webkit-user-select: none;}
                 
            g#name12 svg[state="off"] > svg >  text{
           fill: green;;}
           
           g#name12 svg[state="disable"] > svg >  text{
           fill: gray;;}
           
           g#name12 svg * {
                cursor: pointer;}
          
            g#name12 svg > g > rect{
                fill: transparent;}
                 
            g#name12 svg > rect{     fill: url(#name12__gradienton);} 
            
            g#name12 svg:hover > rect {               
                -webkit-svg-shadow: 3px 3px  3px rgba(0, 0, 0, 0.5);}
         
            g#name12 svg:active > rect {     fill: url(#name12__gradientona);  
                -webkit-svg-shadow: 0px 0px 0px rgba(0, 0, 0, 0);} 
                 
            g#name12 svg > g > rect { 
                stroke: 
            #333;
                stroke-width: 1;}   
                    
            g#name12 svg:hover > g > rect {
                stroke: 
            #555;
                stroke-width: 1;}                  
       
            g#name12 svg[state="off"] > rect {    fill: url(#name12__gradientoff);} 
                    
            g#name12 svg[state="off"]:active > rect {    fill: url(#name12__gradientoffa);}           
           
            g#name12 svg[state="off"] > g > rect {
                stroke: 
            #A00;
                stroke-width: 1;}   
                    
            g#name12 svg[state="off"]:hover > g > rect {        
                stroke: #F00;
                stroke-width: 1;}            
           
            g#name12 svg[state="disable"] > rect, g#name12 svg[state="disable"]:hover > rect,g#name12 svg[state="disable"]:active > rect{ 
                 fill: url(#name12__gradientdsbl); 
                -webkit-svg-shadow: 0px 0px 0px rgba(0, 0, 0, 0);} 
                
            g#name12 svg[state="disable"] > g > rect, g#name12 svg[state="disable"]:hover > g > rect,g#name12 svg[state="disable"]:active > g >rect{
                 stroke-width: 0;
                 stroke: none;}
                 
            g#name12 svg  > svg > text#name12__c1{
                 dysplay: block;}
                 
            g#name12 svg  > svg > text#name12__c2{
                 dysplay: none;}
                 
            g#name12 svg  > svg > text#name12__c3{
                 dysplay: none;}                 
                 
            g#name12 svg[state="disable"]  > svg > text#name12__c1{
                 dysplay: none;}
                 
            g#name12 svg[state="disable"]  > svg > text#name12__c2{
                 dysplay: none;}
                 
            g#name12 svg[state="disable"]  > svg > text#name12__c3{
                 dysplay: block;}
                 
            g#name12 svg[state="off"]  > svg > text#name12__c1{
                 dysplay: none;}
                 
            g#name12 svg[state="off"]  > svg > text#name12__c2{
                 dysplay: block;}
                 
            g#name12 svg[state="off"]  > svg > text#name12__c3{
                 dysplay: none;}      
                 
                 
